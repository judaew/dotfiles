# vim: set ft=sshconfig:

Host judaew-Bitbucket
    HostName bitbucket.org
    User judaew
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/rsa_@@HOSTNAME@@_bitbucket@judaew

Host judaew-Github
    HostName github.com
    User judaew
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/rsa_@@HOSTNAME@@_github@judaew

Host *
    Compression yes
    AddKeysToAgent yes
    UseKeychain yes
